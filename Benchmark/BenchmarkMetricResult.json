{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "metric": {
      "@id": "[BenchmarkMetric](BenchmarkMetric.md)",
      "@type": "@id",
      "type": "object",
      "dldescription": "The metric delivered in this result"
    },
    "participants": {
      "@id": "[Participants](#Participants)",
      "@type": "xsd:number",
      "type": "integer",
      "dldescription": "Sample size or number of participating entities whose data has been aggregated to compute the benchmark Metric"
    },
    "qualityScore": {
      "@id": "[QualityScore](#QualityScore)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": "Combined quality score of the benchmark data on a scale of 0 to 1"
    },
    "mean": {
      "@id": "[Mean](#Mean)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": ""
    },
    "median": {
      "@id": "[Median](#Median)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": ""
    },
    "sum": {
      "@id": "[Sum](#Sum)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": ""
    },
    "stats": {
      "@id": "[BenchmarkMetricStatistics](BenchmarkMetricStatistics.md)",
      "@type": "@id",
      "type": "object",
      "dldescription": "Statistics for this result"
    }
  }
}
