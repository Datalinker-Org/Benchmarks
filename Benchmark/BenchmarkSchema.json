{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "dimensions": {
      "@id": "[BenchmarkDimension](BenchmarkDimension.md)",
      "@type": "@id",
      "type": "array",
      "items": { "type": "object" },
      "@container": "@set",
      "dldescription": "Array of definitions for supported dimensions"
    },
    "metrics": {
      "@id": "[BenchmarkMetric](BenchmarkMetric.md)",
      "@type": "@id",
      "type": "array",
      "items": { "type": "object" },
      "@container": "@set",
      "dldescription": "Array of definitions for supported metrics"
    },
    "benchmarkData": {
      "@id": "[benchmarkData](#benchmarkData)",
      "@type": "@id",
      "type": "string",
      "format": "uri",
      "dltype": "[BenchmarkData](BenchmarkData.md)",
      "dldescription": "URL to Benchmark data to which the dimensions and filters can be applied"
    },
    "organisation": {
      "@id": "http://schema.org/Organization",
      "@type": "@id",
      "type": "object",
      "dldescription": "An organization details defined by schema.org"
    }
  }
}
