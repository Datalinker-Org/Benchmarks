{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "benchmarkMetric": {
      "@id": "[BenchmarkMetric](BenchmarkMetric.md)",
      "@type": "@id",
      "type": "object",
      "dldescription": "Metric without the actual amounts"
    },
    "amount": {
      "@id": "[Amount](#Amount)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": "Actual value of the Metric"
    },
    "participants": {
      "@id": "[Participants](#Participants)",
      "@type": "xsd:number",
      "type": "integer",
      "dldescription": "Sample size or number of participating entities whose data has been aggregated to compute the benchmark Metric"
    },
    "qualityScore": {
      "@id": "[QualityScore](#QualityScore)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": "Combined quality score of the benchmark data on a scale of 0 to 1"
    },
    "mean": {
      "@id": "[Mean](#Mean)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": ""
    },
    "min": {
      "@id": "[Min](#Min)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": ""
    },
    "max": {
      "@id": "[Max](#Max)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": ""
    },
    "median": {
      "@id": "[Median](#Median)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": ""
    },
    "sum": {
      "@id": "[Sum](#Sum)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": ""
    },
    "quartiles": {
      "@id": "[Quartile](#Quartile)",
      "@type": "xsd:decimal",
      "type": "array",
      "items": { "type": "number" },
      "@container": "@set",
      "dldescription": "Ordered array of four quartile values"
    },
    "percentiles": {
      "@id": "[Percentile](#Percentile)",
      "@type": "xsd:decimal",
      "type": "array",
      "items": { "type": "number" },
      "@container": "@set",
      "dldescription": "Ordered array of 10 percentile values"
    },
    "cv": {
      "@id": "[CoefficientOfVariation](#CoefficientOfVariation)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": "coefficient of variation"
    }
  }
}
